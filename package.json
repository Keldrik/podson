{
  "name": "podson",
  "version": "1.2.0",
  "description": "Reads podcast feeds and converts them into json.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "podcast",
    "json",
    "rss",
    "feed",
    "parse",
    "xml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/keldrik/podson"
  },
  "author": "Thomas Wiegold",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "got": "^14.4.5",
    "lodash": "^4.17.21",
    "sax": "^1.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "@types/sax": "^1.2.7",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
}
