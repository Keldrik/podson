{
  "extends": "airbnb-base",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "never"],
    "radix": "off",
    "arrow-body-style": "off",
    "no-multi-assign": "warn",
    "no-param-reassign": "warn",
    "no-plusplus": "off",
    "arrow-parens": "off",
    "no-console": "off"
  }
}
